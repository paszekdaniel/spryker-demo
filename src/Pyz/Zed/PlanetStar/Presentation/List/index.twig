{% extends '@Gui/Layout/layout.twig' %}
{% set widget_title = 'Overview of Stars' %}
{% block head_title widget_title %}
{% block section_title widget_title %}
{% block action %}
    {{ createActionButton('/planet-star/create', 'Create Star' | trans) }}
    {{ createActionButton('/planet/list', 'See Planet' | trans) }}
{% endblock %}
{% block content %}
    {% embed '@Gui/Partials/widget.twig' with { widget_title: 'List of planets' } %}
        {% block widget_content %}
            {{ starTable | raw }}
        {% endblock %}
    {% endembed %}
{% endblock %}
