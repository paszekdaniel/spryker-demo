<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema01.xsd"
          namespace="Orm\Zed\Planet\Persistence"
          package="src.Orm.Zed.Planet.Persistence">
    <table name="pyz_star" phpName="PyzStar">
        <column name="id_star" required="true" type="INTEGER" autoIncrement="true" primaryKey="true" />
        <column name="name" required="true" type="VARCHAR" size="50" />
        <column name="distance" type="FLOAT" />
        <column name="mass_in_suns" type="FLOAT" />
    </table>
</database>
